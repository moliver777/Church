<h1>Magazines</h1>
<%= link_to 'New Magazine', new_magazine_path %>
<br/>
<table>
	<tr>
		<th>Date</th>
		<th>File</th>
		<th></th>
		<th></th>
		<th></th>
		<th></th>
	</tr>
	<% @magazines.each do |magazine| %>
		<tr>
			<td><%= magazine.date %></td>
			<td><%= magazine.filename %>.<%= magazine.original_filename.split(".").last %></td>
			<td><a href="/magazine/<%= magazine.filename %>.<%= magazine.original_filename.split(".").last %>">Download</a></td>
			<td><%= link_to 'Show', magazine %></td>
			<td><%= link_to 'Edit', edit_magazine_path(magazine) %></td>
			<td><a class="rails-delete" data-link="/magazines/delete/<%= magazine.id %>">Destroy</a></td>
		</tr>
	<% end %>
</table>